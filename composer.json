{
    "name": "autorus/car-service",
    "require": {
        "php": "^7.1",
        "arus/php-coding-standard": "^1.0",
        "arus/doctrine-helper-kit": "^1.0",
        "doctrine/annotations": "^1.6",
        "doctrine/orm": "^2.6",
        "monolog/monolog": "^1.24",
        "php-di/php-di": "^6.0",
        "spiral/roadrunner": "^1.3",
        "symfony/console": "^4.2",
        "symfony/validator": "^4.2",
        "zendframework/zend-inputfilter": "^2.10",
        "spiral/php-grpc": "^1.0",
        "google/protobuf": "^3.7",
        "grpc/grpc": "^1.19",
        "google/gax": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Autorus\\CarService\\": "src/"
        }
    },
    "scripts": {
        "test": [
            "phpunit --colors=always --coverage-text",
            "phpcs"
        ]
    },
    "repositories": {
        "arus/doctrine-helper-kit" : {
            "type": "git",
            "url": "git@gitlab.voshod.local:web/doctrine-helper-kit.git",
            "trunk-path": "master"
        }
    }
}
